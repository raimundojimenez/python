def appendsums(lst):
    """
    Repeatedly append the sum of the current last three elements of lst to lst.
    """
    for i in range(1, 25):
        lst.append(lst[-1]+lst[-2]+lst[-3])

sum_three = [0, 1, 2]
appendsums(sum_three)
print sum_three[10]
print sum_three[20]
print sum_three

